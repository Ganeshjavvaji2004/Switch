#include<stdio.h>
void main()
{
    char ch;
    printf("Enter charachter:");
    scanf("%c",&ch);
    switch(ch)
    {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
        case 'A':
        case 'E':
        case 'I':
        case '0':
        case 'U':
        printf("%c is a vowel:",ch);
        break;
        default:printf("It is not a vowel:",ch);
        break;
    }
}
